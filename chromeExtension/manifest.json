{
  "name": "Track-Your-Tabs",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "./popup/TYTLogo128.png",
    "default_popup": "./popup/popup.html"
  },
  "icons": {
    "16": "./popup/TYTLogo16.png",
    "48": "./popup/TYTLogo48.png",
    "128": "./popup/TYTLogo128.png" },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["eventHandler.js"],
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js; object-src 'self'"
}