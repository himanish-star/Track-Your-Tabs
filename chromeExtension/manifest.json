{
  "name": "Track-Your-Tabs",

  "version": "1.0",

  "manifest_version": 2,

  "browser_action": {
    "default_icon": "./popup/logoIcons/TYTLogo128.png",
    "default_popup": "./popup/popup.html"
  },

  "icons": {
    "16": "./popup/logoIcons/TYTLogo16.png",
    "48": "./popup/logoIcons/TYTLogo48.png",
    "128": "./popup/logoIcons/TYTLogo128.png" },

  "permissions": [
    "identity",
    "<all_urls>",
    "history",
    "tabs",
    "storage"
  ],

  "background": {
    "scripts": [
      "backend/routes.js",
      "backend/tabs.js",
      "backend/history.js",
      "backend/identity.js"
    ],
    "persistent": false
  },

  "key": ""
  ,
  "oauth2": {
    "client_id": "655798023590-k65vedqnb38nh0491nuks8etn1p1l8v5.apps.googleusercontent.com",
    "scopes" : [
      "profile"
    ]
  }
}
