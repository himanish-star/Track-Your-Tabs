{
  "name": "Track-Your-Tabs",

  "version": "1.0",

  "manifest_version": 2,

  "browser_action": {
    "default_icon": "./popup/logoIcons/TYTLogo128.png",
    "default_popup": "./popup/popup.html"
  },

  "icons": {
    "16": "./popup/logoIcons/TYTLogo16.png",
    "48": "./popup/logoIcons/TYTLogo48.png",
    "128": "./popup/logoIcons/TYTLogo128.png" },

  "permissions": [
    "tabs",
    "<all_urls>",
    "identity",
    "https://accounts.google.com/*",
    "https://www.googleapis.com/*"
  ],

  "background": {
    "scripts": [
      "backend/routes.js",
      "backend/tabs.js",
      "backend/history.js",
      "backend/login.js"
    ],
    "persistent": false
  },

  "oauth2": {
    "client_id": "939102030430-92ip7jaep1oe9nfnr8279tu84os4kgk4.apps.googleusercontent.com",
    "scopes" : [
      "https://googleapis.com/auth/plus.login","profile","email"
    ]
  },

  "key": ""
}
